		options.play(\"selected\")
	else:
		options.play(\"basic\")
	
	if menu_option == 4:
		credits.play(\"selected\")
	else:
		credits.play(\"basic\")
	
	if menu_option == 5:
		awards.play(\"selected\")
	else:
		awards.play(\"basic\")
	

