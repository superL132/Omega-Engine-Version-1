func _credits_names():
	
	for i in credits.size():
		print(typeof(credits[i]))
		if typeof(credits[i]) == 28:
			var templateLabelDup = templateLabel.duplicate()
			#var templateIconDup = templateIcon.duplicate()
			templateLabelDup.position.y = i * scroll_multiplier
